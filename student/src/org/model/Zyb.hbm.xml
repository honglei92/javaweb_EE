<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC      
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"            
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">       
<hibernate-mapping  package="org.model">        
<class name="Zyb" table="zyb">       
<id name="id" type="java.lang.Integer"> 
<column name="ID"/>    
<generator class="identity"/>             
</id>       
<property name="zym" type="java.lang.String">
<column name="ZYM" length="20" not-null="true"/>
</property>       
<property name="rs" type="java.lang.Integer">
<column name="RS"/>
</property>     
<property name="fdy" type="java.lang.String">
<column name="FDY" length="8"></column>
</property>  
</class>       
</hibernate-mapping>