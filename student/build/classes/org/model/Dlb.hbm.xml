<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC      
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"            
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">       
<hibernate-mapping  package="org.model">        
<class name="Dlb" table="dlb" >       
<id name="id" type="java.lang.Integer"> 
<column name="ID"/>    
<generator class="identity"/>             
</id>       
<property name="xh" type="java.lang.String">
<column name="XH" length="6" not-null="true"/>
</property>       
<property name="kl" type="java.lang.String">
<column name="KL" length="20" not-null="true"/>
</property> 
<property name="role" type="java.lang.String">
<column name="ROLE" length="6" not-null="true"/>
</property>       
</class>       
</hibernate-mapping>