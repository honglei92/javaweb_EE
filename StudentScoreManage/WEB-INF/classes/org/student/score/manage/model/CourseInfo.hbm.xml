<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.student.score.manage.model">
	<class name="CourseInfo" table="CourseInfo">
		<id name="cid" column="cid" type="java.lang.String" length="8">
			<generator class="assigned"></generator>
		</id>
		<property name="cname" column="cname" type="java.lang.String" />
		<property name="term" column="term" type="java.lang.Short" />
		<property name="hours" column="hours" type="java.lang.Short" />
		<property name="credit" column="credit" type="java.lang.Integer" />
	</class>
</hibernate-mapping>