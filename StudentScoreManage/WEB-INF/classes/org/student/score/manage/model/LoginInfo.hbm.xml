<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.student.score.manage.model">
	<class name="LoginInfo" table="LoginInfo">
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="increment"></generator>
		</id>
		<property name="sid" column="sid" type="java.lang.String"
			length="12" not-null="true"></property>
		<property name="password" column="password" type="java.lang.String"
			length="20" not-null="true"></property>
	</class>
</hibernate-mapping>