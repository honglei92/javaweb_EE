	 <form id="searchForm" name="searchForm" action="/jobs/list_" method="post">
        <input type="text" id="search_input" name = "kd"  tabindex="1" value="" placeholder="请输入职位名称或公司名称" />
        <input type="hidden" name="pl" id="plInput" value=""/>
        <input type="hidden" name="gj" id="gjInput" value=""/>
        <input type="hidden" name="xl" id="xlInput" value=""/>
        <input type="hidden" name="yx" id="yxInput" value=""/>
        <input type="hidden" name="gx" id="gxInput" value="" />
        <input type="hidden" name="st" id="stInput" value="" />
                <input type="hidden" name="city" id="cityInput" value="全国"/>
                <input type="submit" id="search_button" value="找工作" />
        <!-- <span class="error" style="display:none;">搜索内容不能为空</span> -->
    </form>
</div>
 <script>
 $(function(){
	$('#search_button').click(function(){
		var keyword = $.trim($('#search_input').val());
		var reg =  /[`~!@\$%\^\&\*\(\)_<>\?:"\{\},\\\/;'\[\]]/g;
		var re = /#/g;
		var r = /\./g;
		var kw = keyword.replace(reg," ");
		if(kw == ''){
			$('#searchForm').attr('action','/jobs/list_所有职位').submit();	
		}else{
			kw = kw.replace(re,'井');
			kw = kw.replace(r,'。');
			$('#searchForm').attr('action','/jobs/list_'+kw).submit();
		}
	}); 
	
	$('#search_input').keydown(function(e){
		if(e.which == 13){
			e.stopPropagation();
			$('#search_button').trigger('click');
		}
	}); 
 });
 </script>
