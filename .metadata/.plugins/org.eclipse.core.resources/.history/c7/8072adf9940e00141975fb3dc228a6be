	{
		for (int i = 0; i <= 20; i++) {
		switch(i%2)
		{
		case 0:System.out.println("学习PHP");
		case 1:System.out.println("Hypertext  preprocessor");
		case 2:System.out.println("超文本标记语言");
		}
	}
	}
    private String functionA(String s)
	{
		return s;
	}
}
